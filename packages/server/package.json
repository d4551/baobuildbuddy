{
  "name": "@bao/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./dist-types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "default": "./src/index.ts"
    },
    "./app": {
      "types": "./dist-types/app.d.ts",
      "default": "./src/app.ts"
    }
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "build:types": "bunx --bun tsc --emitDeclarationOnly --declarationMap false --outDir dist-types",
    "typecheck": "bunx --bun tsc --noEmit",
    "test": "bun test",
    "db:generate": "bun --bun run drizzle-kit generate",
    "db:push": "bun --bun run drizzle-kit push",
    "db:studio": "bun --bun run drizzle-kit studio"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@bogeychan/elysia-logger": "^0.1.10",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/swagger": "^1.3.1",
    "@google/generative-ai": "^0.24.0",
    "@huggingface/inference": "^4.13.12",
    "@bao/shared": "workspace:*",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.25",
    "elysia-rate-limit": "^4.2.0",
    "openai": "^6.22.0",
    "pdf-lib": "^1.17.0",
    "pino": "^10.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.34.48",
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.9",
    "pino-pretty": "^13.0.0",
    "typescript": "^5.9.3"
  }
}
